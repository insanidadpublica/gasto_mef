-- Creado para MySQL 8.0 sobre Ubuntu 22.02
-- Crea una tabla llamada 'data', con una estructura común para los años 2014-2022. Contiene campos varchar dado que las tablas orginales del portal de datos abiertos contienen valores vacíos y nulos.
CREATE TABLE IF NOT EXISTS `data` (
`ANO_EJE` varchar(5) NOT NULL,
`MES_EJE` varchar(5) NOT NULL,
`NIVEL_GOBIERNO` char(1) NOT NULL,
`NIVEL_GOBIERNO_NOMBRE` varchar(50) NOT NULL,
`SECTOR` varchar(5) NOT NULL,
`SECTOR_NOMBRE` varchar(50) NOT NULL,
`PLIEGO` varchar(5) NOT NULL,
`PLIEGO_NOMBRE` varchar(200) NOT NULL,
`SEC_EJEC` varchar(10) NOT NULL,
`EJECUTORA` varchar(50) NOT NULL,
`EJECUTORA_NOMBRE` varchar(200) NOT NULL,
`DEPARTAMENTO_EJECUTORA` varchar(5) NOT NULL,
`DEPARTAMENTO_EJECUTORA_NOMBRE` varchar(50) NOT NULL,
`PROVINCIA_EJECUTORA` varchar(5) NOT NULL,
`PROVINCIA_EJECUTORA_NOMBRE` varchar(100) NOT NULL,
`DISTRITO_EJECUTORA` varchar(5) NOT NULL,
`DISTRITO_EJECUTORA_NOMBRE` varchar(100) NOT NULL,
`SEC_FUNC` varchar(5) NOT NULL,
`PROGRAMA_PPTO` varchar(5) NOT NULL,
`PROGRAMA_PPTO_NOMBRE` varchar(200) NOT NULL,
`TIPO_ACT_PROY` varchar(5) NOT NULL,
`TIPO_ACT_PROY_NOMBRE` varchar(50),
`PRODUCTO_PROYECTO` varchar(10) NOT NULL,
`PRODUCTO_PROYECTO_NOMBRE` varchar(500) NOT NULL,
`ACTIVIDAD_ACCION_OBRA` varchar(10) NOT NULL,
`ACTIVIDAD_ACCION_OBRA_NOMBRE` varchar(300) NOT NULL,
`FUNCION` varchar(5) NOT NULL,
`FUNCION_NOMBRE` varchar(100) NOT NULL,
`DIVISION_FUNCIONAL` varchar(5) NOT NULL,
`DIVISION_FUNCIONAL_NOMBRE` varchar(100) NOT NULL,
`GRUPO_FUNCIONAL` varchar(5) NOT NULL,
`GRUPO_FUNCIONAL_NOMBRE` varchar(100) NOT NULL,
`META` varchar(5) NOT NULL,
`FINALIDAD` varchar(10) NOT NULL,
`META_NOMBRE` varchar(300) NOT NULL,
`DEPARTAMENTO_META` varchar(5) NOT NULL,
`DEPARTAMENTO_META_NOMBRE` varchar(50) NOT NULL,
`FUENTE_FINANCIAMIENTO` varchar(5) NOT NULL,
`FUENTE_FINANCIAMIENTO_NOMBRE` varchar(50) NOT NULL,
`RUBRO` varchar(5) NOT NULL,
`RUBRO_NOMBRE` varchar(100) NOT NULL,
`TIPO_RECURSO` varchar(50) NOT NULL,
`TIPO_RECURSO_NOMBRE` varchar(100) NOT NULL,
`CATEGORIA_GASTO` varchar(5) NOT NULL,
`CATEGORIA_GASTO_NOMBRE` varchar(100) NOT NULL,
`TIPO_TRANSACCION` varchar(5) NOT NULL,
`GENERICA` varchar(5) NOT NULL,
`GENERICA_NOMBRE` varchar(100) NOT NULL,
`SUBGENERICA` varchar(5) NOT NULL,
`SUBGENERICA_NOMBRE` varchar(100) NOT NULL,
`SUBGENERICA_DET` varchar(5) NOT NULL,
`SUBGENERICA_DET_NOMBRE` varchar(100) NOT NULL,
`ESPECIFICA` varchar(5) NOT NULL,
`ESPECIFICA_NOMBRE` varchar(100) NOT NULL,
`ESPECIFICA_DET` varchar(5) NOT NULL,
`ESPECIFICA_DET_NOMBRE` varchar(200) NOT NULL,
`MONTO_PIA` bigint NOT NULL,
`MONTO_PIM` bigint NOT NULL,
`MONTO_CERTIFICADO` decimal(20,2) NOT NULL,
`MONTO_COMPROMETIDO_ANUAL` decimal(20,2) NOT NULL,
`MONTO_COMPROMETIDO` decimal(20,2) NOT NULL,
`MONTO_DEVENGADO` decimal(20,2) NOT NULL,
`MONTO_GIRADO` decimal(20,2) NOT NULL
);
